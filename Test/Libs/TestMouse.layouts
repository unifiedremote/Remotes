<?xml version="1.0" encoding="utf-8"?>
<layout scroll="vertical">
	<row weight="wrap">
		<label id="info" text="" />
	</row>
	<row>
		<grid scroll="vertical">
			<row weight="wrap">
				<button text="moveby" ontap="moveby" />
				<button text="moveto" ontap="moveto" />
			</row>
			<row weight="wrap">
				<button text="down_left" ontap="down_left" />
				<button text="down_middle" ontap="down_middle" />
				<button text="down_right" ontap="down_right" />
			</row>
			<row weight="wrap">
				<button text="up_left" ontap="up_left" />
				<button text="up_middle" ontap="up_middle" />
				<button text="up_right" ontap="up_right" />
			</row>
			<row weight="wrap">
				<button text="click_left" ontap="click_left" />
				<button text="click_m" ontap="click_middle" />
				<button text="click_c" ontap="click_center" />
				<button text="click_right" ontap="click_right" />
			</row>
			<row weight="wrap">
				<button text="double_left" ontap="double_left" />
				<button text="double_middle" ontap="double_middle" />
				<button text="double_right" ontap="double_right" />
			</row>
			<row weight="wrap">
				<button text="vscroll" ontap="vscroll" />
				<button text="hscroll" ontap="hscroll" />
			</row>
			<row weight="wrap">
				<button text="dragbegin" ontap="dragbegin" />
				<button text="dragend" ontap="dragend" />
			</row>
		</grid>
	</row>
</layout>