<?xml version="1.0" encoding="utf-8"?>
<layout>
	<row>
		<text text="text" />
		<text hint="hint" />
	</row>
	<row>
		<text multiline="true" />
	</row>
	<row>
		<text textalign="left" text="left" />
		<text textalign="center" text="center" />
		<text textalign="right" text="right" />
	</row>
	<row>
		<text text="down" ondown="down" />
		<text text="up" onup="up" />
		<text text="change" onchange="change" />
		<text text="done" ondone="done" />
	</row>
</layout>