<?xml version="1.0" encoding="utf-8"?>
<layout onLaunch="launch" onVolumeUp="scroll_up" onVolumeDown="scroll_down" color="blue" >
	<tabs onchange="tab_index">
		<tab text="Firefox" color="orange" >			
			<grid>
				<row>
					<button text="Next Post" onTap="Next_Post" />								
					<button image="like_logo.png" onTap="Like_Post" />	
					<button text="Previous Post" onTap="Prev_Post" />
				</row>
				<row>
					<button text="Friend Request" onTap="Friend_Request" />
					<button text="Messages" onTap="Messages" />
					<button text="Notification Center" onTap="Note_Center" />
				</row>
				<row>
					<button text="Home Page" onTap="Home_Page" />
					<button text="My Profile Page" onTap="Profile_Page" />
					<button text="New Message" onTap="New_Message" />
				</row>
				<row>
					<button text="Esc" ontap="nav_escape" />
					<button icon="up" ontap="nav_up" />
					<button icon="backspace" ontap="nav_back" />
				</row>
				<row>
					<button icon="left" ontap="nav_left" />
					<button icon="select" ontap="nav_enter" />
					<button icon="right" ontap="nav_right" />
				</row>
				<row>
					<button text="Tab" ontap="nav_tab" />
					<button icon="down" ontap="nav_down" />
					<button text="Menu" ontap="nav_menu" />
				</row> 				
			</grid>			
		</tab>
	
