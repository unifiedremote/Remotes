<?xml version="1.0" encoding="utf-8"?>
<layout onlaunch="launch" onVolumeUp="volume_up" onVolumeDown="volume_down" color="#FF9500">
	<tabs>
		<tab text="Main">
			<row>
				<button icon="back" ontap="back" />
				<button color="#525252" icon="up" ontap="up" />
				<button icon="playpause" ontap="play_current" />
			</row>
			<row>
				<button color="#525252" icon="left" ontap="left" />
				<button color="#525252" icon="select" ontap="select" />
				<button color="#525252" icon="right" ontap="right" />
			</row>
			<row>
				<button icon="previous" ontap="previous" />
				<button color="#525252" icon="down" ontap="down" />
				<button icon="next" ontap="next" />
			</row>
			<row>
				<button text="Menu" ontap="menu" />
				<button text="Home" ontap="home" />
				<button text="OSD" ontap="osd" />
				<button text="Info" ontap="info" />
			</row>
		</tab>
		<tab text="Play">
			<row>
				<button icon="rwd" ontap="rewind" />
				<button icon="ff" ontap="forward" />
			</row>
			<row>
				<button icon="previous" ontap="previous" />
				<button icon="next" ontap="next" />
			</row>
			<row>
				<button icon="stop" ontap="stop" />
				<button icon="playpause" ontap="play_pause" />
			</row>
		</tab>
	</tabs>
</layout>
