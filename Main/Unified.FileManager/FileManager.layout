<?xml version="1.0" encoding="utf-8"?>
<layout onLaunch="launch" onVolumeUp="volume_up" onVolumeDown="volume_down">
  <grid>
    <row weight="wrap">
      <button icon="back" onTap="back" />
      <button icon="more" onTap="goto" />
      <button icon="arrowup" onTap="up" />
      <button icon="home" onTap="home" />
      <button icon="refresh" onTap="refresh" />
    </row>
    <row>
      <list id="lst" onTap="onItem" onHold="onHolditem" />
    </row>
  </grid>
</layout>